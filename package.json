{
  "name": "static-i18n",
  "description": "Utility to translate static HTML files.",
  "version": "0.1.16",
  "author": "Daniel Perez",
  "bin": {
    "static-i18n": "bin/static-i18n.js"
  },
  "bugs": {
    "url": "https://github.com/claudetech/node-static-i18n/issues"
  },
  "config": {
    "blanket": {
      "pattern": "src",
      "loader": "./node-loaders/coffee-script",
      "data-cover-never": "node_modules"
    }
  },
  "dependencies": {
    "async": "^2.1.5",
    "cheerio": "^0.22.0",
    "fs-extra": "^2.0.0",
    "glob": "^7.1.1",
    "i18next": "^7.0.1",
    "js-yaml": "^3.2.2",
    "lodash": "^4.17.4",
    "minimist": "^1.1.0",
    "string": "^3.3.3"
  },
  "devDependencies": {
    "coffee-script": "^1.8.0",
    "expect.js": "^0.3.1",
    "grunt": "^1.0.1",
    "grunt-contrib-coffee": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-mocha-cov": "^0.4.0",
    "tmp": "^0.0.31"
  },
  "homepage": "https://github.com/claudetech/node-static-i18n",
  "keywords": [
    "html",
    "i18n",
    "internationalization"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/claudetech/node-static-i18n"
  },
  "scripts": {
    "prepublish": "grunt build",
    "test": "grunt test"
  }
}
